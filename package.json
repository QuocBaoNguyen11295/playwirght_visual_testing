{
  "name": "playwirght_visual_testing",
  "version": "1.0.0",
  "description": "Visual testing with playwright",
  "main": "index.js",
  "scripts": {
    "pw:test": "playwright test --config=visual.config.ts --reporter=html --update-snapshots",
    "pw:show-report" : "playwright show-report",
    "pw:chrome": "playwright test --config=visual.config.ts --reporter=html --project=chromium --update-snapshots",
    "pw:firefox": "playwright test --config=visual.config.ts --reporter=html --project=firefox --update-snapshots",
    "pw:webkit": "playwright test --config=visual.config.ts --reporter=html --project=webkit --update-snapshots"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuocBaoNguyen11295/playwirght_visual_testing.git"
  },
  "keywords": [
    "visual_testing",
    "playwright",
    "ts"
  ],
  "author": "Quoc Bao Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/QuocBaoNguyen11295/playwirght_visual_testing/issues"
  },
  "homepage": "https://github.com/QuocBaoNguyen11295/playwirght_visual_testing#readme",
  "dependencies": {
    "@playwright/test": "^1.26.1",
    "prettier": "^2.7.1"
  }
}
